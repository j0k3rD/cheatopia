{
    "manifest_version": 3,
    "name": "CheatExtension",
    "description": "CheatExtension v1.0",
    "version": "1.0",
    "icons": {
        "128": "hacker.png"
    },
    "permissions": [
        "clipboardWrite",          
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.google.com/*",
        "https://virtual.um.edu.ar/*"
    ]
}